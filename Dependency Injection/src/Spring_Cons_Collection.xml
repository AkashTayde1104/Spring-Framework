<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="s" class="com.constructor.collection.Student">
		<constructor-arg>
			<list>
			
				<value>101</value>
				<value>RAM</value>
				<value>Pune</value>

			</list>
		</constructor-arg>

		<constructor-arg>
			<set>
				<value>Ritesh</value>
				<value>Ram</value>

			</set>
		</constructor-arg>

		<constructor-arg>
			<map>
				<entry key="101" value="Ramesh"></entry>
				<entry key="102" value="Rahul"></entry>

			</map>

		</constructor-arg>
	</bean>



</beans>