<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
   <bean id="addres" class="com.tech.autodetect.Address">
       <constructor-arg value="Moshi"></constructor-arg>
       <constructor-arg value="Pune"></constructor-arg>
       <property name="area_Name" value="Moshi"></property>
       <property name="city_Name" value="Pune"></property>
   </bean>

<bean id="s" class="com.tech.autodetect.Student" autowire="autodetect">
  <constructor-arg value="101" type="int"></constructor-arg>
  <constructor-arg value="Sachin"></constructor-arg>
  <property name="s_Id" value="101"></property>
  <property name="s_Name" value="Sameer"></property>
 
</bean>

</beans>